      "application_layer/*": ["./src/js/application_layer/*"],
      "comparators": ["./src/js/comparators"],
      "comparators/*": ["./src/js/comparators/*"],
      "components": ["./src/js/components"],
      "components/*": ["./src/js/components/*"],
      "config": ["./src/js/config"],
      "config/*": ["./src/js/config/*"],
      "const": ["./src/js/const"],
