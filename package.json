  "scripts": {
    "start": "DISABLE_ABC_LINT=true HTTPS=true PORT=4000 craco start",
    "build": "DISABLE_ABC_LINT=true env-cmd -f .env craco --max_old_space_size=8192 build",
    "build:stats": "DISABLE_ABC_LINT=true env-cmd -f .env craco --max_old_space_size=8192 build --stats",
    "build:production": "DISABLE_ABC_LINT=true env-cmd -f .env.production craco build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "test": "craco test --watchAll=false --testURL=https://app.edge.mazepay.io/v1",
    "test:watch": "craco test --testURL=https://app.edge.mazepay.io/v1",
    "test:coverage": "npm test -- --coverage --reporters=./node_modules/jest-html-reporters --watchAll=false",
    "cypress:open": "cross-env TZ=Europe/Copenhagen npx cypress open",
    "cypress:vr:diff": "find ./cypress/snapshots -type d -name '__diff_output__'",
    "cypress:vr:diff:clear": "find ./cypress/snapshots -type d -name '__diff_output__' -exec rm -rf {} +",
    "cypress:vr:rollback": "git checkout origin/develop -- cypress/snapshots",
    "cypress": "cross-env TZ=Europe/Copenhagen npx cypress run --headless --browser electron",
    "cypress:test": "cross-env TZ=Europe/Copenhagen npx cypress run --headless --browser electron",
    "cypress:update": "cross-env TZ=Europe/Copenhagen npx cypress run --headless --browser electron --env updateSnapshots=true",
    "cypress:component:test": "yarn cypress:update --component --spec 'cypress/component/**'",
    "cypress:vr:test": "yarn cypress:test --spec '**/*.visual.cy.js,**/*.visual.cy.ts'",
    "cypress:vr:update": "yarn cypress:update --spec '**/*.visual.cy.js,**/*.visual.cy.ts'",
    "cypress:stub:test": "yarn cypress:test --spec '**/*.stub.cy.js,**/*.stub.cy.ts'",
    "cypress:e2e:test": "yarn cypress:test --spec '**/*.e2e.cy.js'",
    "cypress:report": "generate-mochawesome-report",
    "cypress:test:firefox": "cross-env TZ=Europe/Copenhagen npx cypress run --headless --browser firefox",
    "cypress:update:firefox": "cross-env TZ=Europe/Copenhagen npx cypress run --headless --browser firefox --env updateSnapshots=true",
    "cypress:vr:test:firefox": "yarn cypress:test:firefox --spec '**/*.visual.cy.js'",
    "cypress:vr:update:firefox": "yarn cypress:update:firefox --spec '**/*.visual.cy.js'",
    "gql": "./src/bash/changeset_version.sh && yarn gql:plugins && yarn gql:fragments && yarn gql:schema && yarn gql:format && yarn gql:sync && yarn gql:format:hashes",
    "gql:format": "prettier --write src/generated",
    "gql:format:hashes": "prettier --write src/generated/OperationStoreClient.js",
    "gql:fragments": "graphql-codegen --config src/graphql/codegen_plugins/plugins.yml",
    "gql:plugins": "tsc -p src/graphql/codegen_plugins/tsconfig.json",
    "gql:schema": "graphql-codegen --config codegen.yml",
    "gql:sync": "./src/bash/store_graphql_operation.sh https://app.edge.mazepay.io/graphql/frontend/sync bac7d5a9cf4fe9a508cf0e583e07e17e83ebc93aeff38215ea1121f4b1061ef9",
    "gql:sync:release": "./src/bash/store_graphql_operation.sh $GRAPHQL_SYNC_URL $GRAPHQL_SYNC_SECRET",
    "locales:prepare": "find src/locales -name '*.json' | while read file ; do gsed -z 's/\\n*$//g' $file > temp.json && mv temp.json $file ; done",
    "prepare": "husky install ; chmod +x src/bash/* || :",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "playwright:component:test": "PLAYWRIGHT_SNAPSHOTS=true playwright test -c playwright-ct.config.ts",
    "playwright:component:test:ui": "playwright test -c playwright-ct.config.ts --ui --debug",
    "playwright:component:update": "PLAYWRIGHT_SNAPSHOTS=true playwright test -c playwright-ct.config.ts --update-snapshots",
    "playwright:test": "PLAYWRIGHT_SNAPSHOTS=true npx playwright test",
    "playwright:update": "PLAYWRIGHT_SNAPSHOTS=true npx playwright test --update-snapshots",
    "playwright:test:ui": "npx playwright test --ui --debug",
    "playwright:vr:test": "PLAYWRIGHT_SNAPSHOTS=true npx playwright test visual",
    "playwright:vr:update": "PLAYWRIGHT_SNAPSHOTS=true npx playwright test visual --update-snapshots",
    "playwright:stub:test": "PLAYWRIGHT_SNAPSHOTS=true npx playwright test stub",
    "playwright:report": "npx playwright show-report",
    "storybook": "DISABLE_ABC_LINT=true storybook dev -p 6006",
    "build-storybook": "DISABLE_ABC_LINT=true storybook build",
    "fixtures": "node ./cypress/fixtures/generate.js && prettier --write ./cypress/fixtures/fixtures.ts"
